{
  "name": "sales-chat-openai-rag",
  "version": "1.0.0",
  "description": "Enhanced RAG system for the sales team",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node scripts/setup_test_env.js && ./scripts/run_tests.sh",
    "test:focus": "node scripts/setup_test_env.js && node scripts/run_focused_test.js",
    "test:query": "node scripts/setup_test_env.js && node scripts/run_focused_test.js query_analysis",
    "test:reranking": "node scripts/setup_test_env.js && node scripts/run_focused_test.js reranking",
    "test:embedding": "node scripts/setup_test_env.js && node scripts/run_focused_test.js embedding_prep",
    "test:visual": "node scripts/setup_test_env.js && node scripts/run_focused_test.js visual_answer_generator",
    "test:contextual": "node scripts/setup_test_env.js && node scripts/run_focused_test.js contextual_retrieval",
    "test:multimodal": "node scripts/setup_test_env.js && node scripts/run_focused_test.js multimodal",
    "test:categories": "jest utils/__tests__/documentCategories.test.ts",
    "test:metadata": "jest types/__tests__/metadata.test.ts",
    "test:smart-ingestion": "ts-node scripts/test_smart_ingestion.ts",
    "build:test": "tsc -p scripts/tsconfig.test.json",
    "evaluate:baseline": "ts-node scripts/run_baseline_evaluation.ts",
    "evaluate:contextual": "TS_NODE_PROJECT=./tsconfig.json node --loader ts-node/esm scripts/tests/run_test_evaluation.ts",
    "evaluate:simple": "node scripts/tests/evaluate-simple.js",
    "calculate:corpus-stats": "ts-node scripts/calculate_corpus_statistics.ts",
    "test:bm25": "ts-node scripts/test_bm25.ts",
    "test:hybrid": "ts-node scripts/test_hybrid_search.ts",
    "test:old-reranking": "ts-node scripts/test_reranking.ts",
    "test:query-expansion": "ts-node scripts/test_query_expansion.ts",
    "test:all": "ts-node scripts/test_all_enhancements.ts",
    "transform:workstream-data": "ts-node scripts/transform_workstream_data.ts",
    "test:old-query": "ts-node scripts/test_query.ts",
    "test:workstream": "ts-node scripts/test_workstream_queries.ts",
    "fix:hybrid-search": "ts-node scripts/fix_hybrid_search.ts",
    "test:entity-handling": "ts-node scripts/test_entity_handling.ts",
    "test:entity-handling-fixed": "ts-node -P scripts/tsconfig.entity-test.json scripts/test_entity_handling.ts",
    "test:entity-js": "node scripts/entity_test.js",
    "build": "next build",
    "build:next": "next build",
    "clean:build": "rm -rf dist && next build",
    "start": "next start",
    "dev": "next dev",
    "dev:ts-ignore": "DISABLE_ESLINT_PLUGIN=true TS_NODE_TRANSPILE_ONLY=true next dev",
    "lint": "eslint . --config eslint.config.js",
    "test:hybrid-search": "ts-node scripts/test_hybrid_search_direct.ts",
    "reset:process": "ts-node scripts/reset_and_process.ts",
    "process:failed": "ts-node scripts/process_failed_docs.ts",
    "test:direct": "ts-node scripts/direct_search_test.ts",
    "process:visual": "node scripts/processVisual.js",
    "test:old-multimodal": "node scripts/tests/test_multimodal.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.0",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-table": "^8.13.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.16",
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "chart.js": "^4.3.0",
    "cheerio": "^1.0.0",
    "commander": "^11.0.0",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "formidable": "^3.5.2",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.287.0",
    "mammoth": "^1.9.0",
    "next": "^13.5.4",
    "node-fetch": "^3.3.2",
    "openai": "^4.11.0",
    "p-limit": "^6.2.0",
    "path-browserify": "^1.0.1",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.6.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-intersection-observer": "^9.5.0",
    "react-markdown": "^10.1.0",
    "react-table": "^7.8.0",
    "recharts": "^2.15.2",
    "tesseract.js": "^6.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "esbuild": "^0.25.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.4"
  },
  "author": "",
  "license": "ISC"
}
